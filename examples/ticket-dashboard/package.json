{
  "name": "@autotask-examples/ticket-dashboard",
  "version": "1.0.0",
  "description": "Real-time ticket management dashboard with SLA tracking and analytics for Autotask PSA",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:build": "docker build -t autotask-ticket-dashboard .",
    "docker:run": "docker run -p 3002:3002 --env-file .env autotask-ticket-dashboard"
  },
  "keywords": [
    "autotask",
    "ticket-management",
    "dashboard",
    "sla-tracking",
    "real-time",
    "analytics"
  ],
  "author": "Autotask Examples",
  "license": "MIT",
  "dependencies": {
    "autotask-node": "file:../../..",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.1",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "bull": "^4.11.3",
    "redis": "^4.6.7",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/socket.io": "^3.0.2",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/node-cron": "^3.0.8",
    "@types/jest": "^29.5.3",
    "@types/pg": "^8.10.2",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}